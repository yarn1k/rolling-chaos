      \u043F\u043E\u0445\u043E\u0436\u0435\u043C \u043D\u0430 \u0437\u043B\u043E\u0432\u0435\u0449\u0435\u0435
      \u043F\u043B\u0430\u0442\u043E, \u043F\u0440\u0435\u0434\u043B\u0430\u0433\u0430\u0435\u0442
      \u0435\u043C\u0443 \u0438\u0437\u043C\u0435\u043D\u0438\u0442\u044C \u043C\u0438\u0440.
      \u0412\u043C\u0435\u0441\u0442\u0435 \u0441 \u0442\u0435\u043C \u0441\u0442\u0430\u043B
      \u0438\u0441\u043F\u044B\u0442\u044B\u0432\u0430\u0442\u044C \u0441\u0442\u0440\u0430\u043D\u043D\u044B\u0435
      \u0441\u0438\u043C\u043F\u0442\u043E\u043C\u044B - \u0435\u0433\u043E \u043A\u043E\u0436\u0430
      \u043D\u0430\u0447\u0430\u043B\u0430 \u0448\u0435\u043B\u0443\u0448\u0438\u0442\u044C\u0441\u044F,
      \u0430 \u0441\u0430\u043C \u043E\u043D \u0432\u0441\u0451 \u0431\u043E\u043B\u0435\u0435
