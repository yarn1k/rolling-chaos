      \u0441\u0442\u0440\u0438\u0436\u0435\u043D\u043D\u044B\u0435 \u0432\u043E\u043B\u043E\u0441\u044B,
      \u0435\u0441\u0442\u044C \u0443\u0441\u044B."
    ShortBiography: "\u0412\u044B\u0445\u043E\u0434\u0435\u0446 \u0438\u0437 \u0441\u0435\u043C\u044C\u0438
      \u0431\u044B\u0432\u0448\u0438\u0445 \u043F\u043B\u0430\u043D\u0442\u0430\u0442\u043E\u0440\u043E\u0432
      \u042E\u0433\u0430, \u043F\u043E\u043B\u0443\u0447\u0438\u043B \u043E\u0431\u0440\u0430\u0437\u043E\u0432\u0430\u043D\u0438\u0435
      \u0445\u0438\u0440\u0443\u0440\u0433\u0430, \u043F\u0440\u043E\u0448\u0451\u043B
      \u0432\u043E\u0439\u043D\u0443, \u043F\u043E\u0441\u043B\u0435 \u043A\u043E\u0442\u043E\u0440\u043E\u0439
      \u043F\u044B\u0442\u0430\u0435\u0442\u0441\u044F \u0438\u0437\u0431\u0430\u0432\u0438\u0442\u044C\u0441\u044F
      \u043E\u0442 \u0441\u0432\u043E\u0438\u0445 \u043F\u0441\u0438\u0445\u043E\u043B\u043E\u0433\u0438\u0447\u0435\u0441\u043A\u0438\u0445
      \u0442\u0440\u0430\u0432\u043C. \u041F\u043E\u0441\u043B\u0435 \u0434\u043E\u043B\u0433\u043E\u0435
      \u0432\u0440\u0435\u043C\u044F \u0436\u0438\u043B \u0432 \u0431\u0435\u0434\u043D\u043E\u0441\u0442\u0438,
      \u043F\u0435\u0440\u0435\u0431\u0438\u0432\u0430\u044F\u0441\u044C \u0441\u043B\u0443\u0447\u0430\u0439\u043D\u044B\u043C\u0438
      \u043F\u043E\u0434\u0440\u0430\u0431\u043E\u0442\u043A\u0430\u043C\u0438 \u0432
      \u0427\u0438\u043A\u0430\u0433\u043E, \u043F\u043E\u043A\u0430 \u043E\u0434\u043D\u0430\u0436\u0434\u044B
      \u043D\u0435 \u0441\u043F\u0430\u0441 \u0436\u0438\u0437\u043D\u044C \u0440\u0435\u0434\u0430\u043A\u0442\u043E\u0440\u0443
      \u0433\u0430\u0437\u0435\u0442\u0443. \u041F\u043E\u0441\u043B\u0435 \u044D\u0442\u043E\u0433\u043E
      \u0442\u043E\u0442 \u0432\u0437\u044F\u043B \u0435\u0433\u043E \u043D\u0430
      \u0434\u043E\u043B\u0436\u043D\u043E\u0441\u0442\u044C \u0440\u0435\u043F\u043E\u0440\u0442\u0435\u0440\u0430
      \u0438 \u043A\u043E\u043B\u0443\u043C\u043D\u0438\u0441\u0442\u0430, \u0433\u0434\u0435
      \u0442\u043E\u0442 \u043F\u0440\u043E\u0441\u043B\u0430\u0432\u0438\u043B\u0441\u044F
      \u0441\u0432\u043E\u0438\u043C\u0438 \u0441\u0442\u0430\u0442\u044C\u044F\u043C\u0438
      \u0438\u0437\u043E\u0431\u043B\u0438\u0447\u0430\u044E\u0449\u0438\u043C\u0438
      \u0431\u0435\u0434\u043D\u043E\u0441\u0442\u044C \u0438 \u043A\u0440\u0438\u043C\u0438\u043D\u0430\u043B
      \u0432 \u043D\u0438\u0449\u0438\u0445 \u0440\u0430\u0439\u043E\u043D\u0430\u0445
      \u0427\u0438\u043A\u0430\u0433\u043E.  \u0412 \u043F\u043E\u0441\u043B\u0435\u0434\u043D\u0438\u0435
      \u0433\u043E\u0434\u044B \u0441\u0442\u0430\u043B \u0441\u0442\u0440\u0430\u0434\u0430\u0442\u044C
