    \u043E\u0441\u043D\u043E\u0432\u043D\u044B\u0445 \u0441\u0432\u043E\u0439\u0441\u0442\u0432
    \u043C\u043E\u0440\u0444\u0438\u043D\u0430 - \u0438\u0437\u0431\u0430\u0432\u043B\u0435\u043D\u0438\u0435
    \u043E\u0442 \u0431\u043E\u043B\u0438. \u0412\u043E\u0442 \u0431\u044B \u043C\u043D\u0435
    \u043D\u0430\u0439\u0442\u0438 \u043C\u043E\u0440\u0444\u0438\u0439 \u0434\u043B\u044F
    \u043C\u043E\u0435\u0439 \u0438\u0437\u043C\u0443\u0447\u0435\u043D\u043D\u043E\u0439
    \u0434\u0443\u0448\u0438; - \u041D\u0435\u043D\u0430\u0432\u0438\u0436\u0443
    \u0432\u0441\u0435\u0445 \u044D\u0442\u0438\u0445 \u043C\u0440\u0430\u0437\u0435\u0439.
    \u042F \u0431\u044B \u0441\u043C\u043E\u0433 \u0438\u0441\u0446\u0435\u043B\u0438\u0442\u044C
    \u0438\u0445 \u043E\u0442 \u0441\u0442\u0440\u0430\u0448\u043D\u043E\u0433\u043E
